{
  "compilerOptions": {
    "target": "ES2020",
    "downlevelIteration": true,
    "module": "esnext",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "moduleResolution": "Node",
    "composite": true,
    "declarationMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext", "dom", "webworker"],
    "skipLibCheck": true
  }
}
